memory[4] = 24'b01001_000_001_010_0000000000; // sub AC = R1 - R2
memory[5] = 24'b11000_111_000_000_0000000000; // jump if AC <= 0		
memory[6] = 24'b01100_010_000_000_0000000000; // inc R2
memory[7] = 24'b01000_011_011_010_0000000000; // add R3 = R3 + R2
memory[8] = 24'b00110_111_000_000_0000000011; // R7 = 3
memory[9] = 24'b11001_111_000_000_0000000000; // jump R7
		
